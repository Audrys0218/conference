<!DOCTYPE html>
<html>
<head>
    <title>Login form</title>
    #parse("blocks/head.vm")
    <link rel="stylesheet" href="/resources/css/style.css" type="text/css">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
</head>


<body>
    #parse("blocks/nav.vm")
    <div class="wrapper">
        <form class = "form2">

            <div class="formtitle">Create conference</div>

            <div class="input">
                <div class="inputtext">Name:</div>
                <div class="inputcontent">
                    <input type="text">
                </div>
            </div>

            <div class="input">
                <div class="inputtext">Starts:</div>
                <div class="inputcontent">
                    <input type="text">
                </div>
            </div>

            <div class="input">
                <div class="inputtext">Ends:</div>
                <div class="inputcontent">
                    <input type="text">
                </div>
            </div>

            <div class="input">
                <div class="inputtext">Category:</div>
                <div class="inputcontent">
                    <input type="text">
                </div>
            </div>
            <div class="input">
                <div class="inputtext">City/Town</div>
                <div class="inputcontent">
                    <input type="text">
                </div>
            </div>

            <div class="input">
                <div class="inputtext">Street</div>
                <div class="inputcontent">
                    <input type="text">
                </div>
            </div>
            <div class="input">
                <div class="inputtext">Description</div>
                <div class="textareaWrapper">
                    <textarea class="textarea"></textarea>
                </div>
                <div class="clearfix" class></div>
            </div>

            <div class="buttons">

                <input class="orangebutton" type="submit" value="Send" />

            </div>
        </form>
    <div>
</body>
</html>